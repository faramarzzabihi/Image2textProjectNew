{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true,
            "console": "integratedTerminal",
            "env": {
                "DJANGO_SETTINGS_MODULE": "image_to_text_project.settings"
            },
            "stopOnEntry": false,
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "-r",
                "image_to_text_project/settings"
            ],
            "runtimeVersion": "3.8.2",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "debugOptions": [
"RedirectOutput"
]
}
]
}